<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE document SYSTEM "../units/schema/language.dtd">
<document>
	<cms:var name="_DOCTYPE"/>
	<html lang="$language" cms:eval="lang">
		<head>
			<title><cms:text constant="site_title"/><cms:raw text=" - "/><cms:text constant="eguage_page_label"/></title>
			<link href='https://fonts.googleapis.com/css?family=Ubuntu|Ultra' rel='stylesheet' type='text/css'/>
			<cms:collection include="page_control"/>
			<cms:module name="head_tag" action="print_tag"/>
		</head>

		<body class="$language_rtl ? 'rtl' : 'ltr'" cms:eval="class">
			<!-- Header -->
			<cms:template file="parts/header.xml"/>

			<section id="about_egauge">
				<h2><cms:text constant="title_egauge"/></h2>
			</section>

			<section id="eguage_details">
				<cms:module
					name="articles"
					action="show"
					text_id="ethernet"
					template="parts/basic_article.xml"
					/>

				<cms:module
					name="articles"
					action="show"
					text_id="eguage_features"
					template="parts/basic_article.xml"
					/>	

				<cms:module
					name="articles"
					action="show"
					text_id="specification"
					template="parts/basic_article.xml"
					/>		
			</section>



			<!-- Footer -->
			<cms:template file="parts/footer.xml"/>
		</body>
	</html>
</document>
